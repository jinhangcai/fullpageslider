.section4{
  position relative
  background url('../images/consociation-bg.jpg') no-repeat center
  background-size: cover;
  .consociation-cen{
    width 1000px
    margin 0 auto
    height 100%
    position relative
    .consociation-img{
      width 747px
      height 131px
      margin 200px 0 0 0
    }
    .content{
      width 830px
      margin 0 auto
      overflow hidden
      position absolute
      left 10%
      top  31%
      img{
        display block
        width 200px
        height 200px
        margin 28px 60px 0 0
        float left
        position relative
        z-index 2
      }
      img:nth-child(3){
        margin-right 0
      }
      img:nth-child(6){
        margin-right 0
      }
    }
  }
  .content-bg{
    background url('../images/consociation-bg.png') no-repeat
    width 976px
    height 971px
    position absolute
    right 0
    bottom 0
    background-size 100%
  }
}
.section5{
  position relative
  background url('../images/touch.jpg') no-repeat center
  background-size: cover;
  .bg{
    height 100%
    position relative
    .line{
      margin 174px auto 0
      width 100%
      height 1px
    }
    .cen{
      width 1000px
      height 100%
      margin 0 auto 0
      position relative
      //background #f6f6f6
      //background rgba(246,246,246,0.82)
      .section5-abs{
        position absolute
        width 100%
        left 0
        top 34%
      }
      .touch-div{
        position relative
        z-index 3
        .touch-me-img{
          width 671px
          height 131px
          margin 0
        }
      }

      p{
        color #333333
        font-size 21px
        padding-top 25px
        width 604px
        margin 0 auto
        text-align left
      }
      .phone{
        font-size 31px
        position relative
        text-align left
        margin 50px auto 0
        padding-left 70px
        width 533px
        img{
          position absolute
          left 0
          top 20px
          width 45px
          height 45px
        }
      }
      .email{
        padding-top 13px
      }
      .address{
        padding-bottom 200px
        padding-top 10px
        .address1{
          display block
          padding-left 105px
        }
      }

    }
  }
  .footer{
    text-align center
    position absolute
    left 0
    bottom 0
    height 60px
    line-height 60px
    font-size 14px
    color #666
    width 100%
    z-index 2
    background #eeeeee
  }
}
