.section3{
  position relative
  background url('../images/refer.jpg') no-repeat center
  background-size: cover;
  .refer-cen{
    width 1000px
    margin 0 auto
    height 100%
    position relative
    .refer-img{
      width 1000px
      height 131px
      margin-top 200px
    }
    .content{
      width 100%
      background #fff
      overflow hidden
      position absolute
      left 0
      top 35%
      .refer-car{
        width 313px
        height 528px
        display inline-block
        float left
      }
      .refer-title{
        width 657px
        padding-left 30px
        float left
