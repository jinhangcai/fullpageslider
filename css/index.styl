* {
  margin: 0;
  padding: 0;
}
body,
div,
dl,
dt,
dd,
ul,
ol,
li,
h1,
h2,
h3,
h4,
h5,
h6,
pre,
code,
form,
fieldset,
legend,
input,
textarea,
p,
blockquote,
th,
td,
hr,
button,
article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section {
  margin: 0;
  padding: 0;
}
html,
body {
  font-family:'微软雅黑';
}
table {
  border-collapse: collapse;
  border-spacing: 0;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  font-size: 100%;
  font-weight: 500;
}
ol,
ul {
  list-style: none;
}
fieldset,
img {
  border: 0;
}
input,
select,
textarea {
  font-size: 100%;
}
ins,
a {
  text-decoration: none;
}
.section{
  overflow hidden
}
.wrap{
  width: 100%;
  height: 100%;
  .head {
    position fixed
    left 0
    top 0
    z-index 9999
    width: 100%;
    height: 90px;
    background: #67ab52;
    .headcen{
      width 100%
      position relative
      margin 0 auto
      .head-left{
        //float left
        //overflow hidden
        //position relative

        .navig{
          width 680px
          margin 0 auto
          padding 37px 0 0 0
          #menu{
            margin-left 26px
            li:nth-last-child(1){
              margin-right 0
            }
          }
          li{
            display inline-block
            margin-right 40px
            height 53px
            position relative
            a{
              color #fff
              font-size 18px
              display block
              width 100%
              height 53px
            }
          }
          .active{
            &:before{
              content ''
              width 100%
              height 3px
              position absolute
              bottom 0
              background #b02d2d
            }
          }
          .index-red{
            &:before{
              content ''
              width 70px
              height 3px
              position absolute
              bottom 0
              left -14px
              background #b02d2d
            }
          }
        }

      }
      .head-list1{
        position absolute
        top 50px
        left 73px
        .logo-img{
          width 343px
          height 25px
          background url('../images/logo.png') no-repeat;
          background-size 100%
          position relative
          left 0
          bottom 12px
          display inline-block
        }
        .logo{
          color #fff
          font-size 26px
          padding 0 0 0 10px
        }
      }
      .head-list2{
        width 680px
        margin 0 auto
      }
      .head-list3{
        .logo-phone{
          background url('../images/phone.png') no-repeat;
          background-size auto 100%
          position absolute
          right 2%
          bottom 23px
          width 189px
          height 31px
        }
        span{
          display inline-block
          padding 47px 0 0 50px
          color #fff
          font-size 30px
        }
      }
    }

  }
}
