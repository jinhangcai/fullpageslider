  background-size: cover;
}
.fp-controlArrow{
  display none
}
#fp-nav ul li, .fp-slidesNav ul li{
  width 35px
  height 5px
}
#fp-nav ul li a span, .fp-slidesNav ul li a span{
  width 35px
  height 5px
  background #fff
  border-radius 0
  border none
}
#fp-nav ul li a.active span, .fp-slidesNav ul li a.active span{
  background #579b42
  border none
}
